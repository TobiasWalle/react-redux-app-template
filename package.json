{
  "name": "react-redux-app",
  "version": "1.0.0",
  "description": "My first react app with redux. Following the tutorial on https://medium.com/@rajaraodv/step-by-step-guide-to-building-react-redux-apps-using-mocks-48ca0f47f9a#.loitituz1",
  "main": "index.js",
  "scripts": {
    "start": "npm run --silent start:dev",
    "start:dev": "node ./scripts/build.js development --watch --target server --after-first-run \"node ./dist/server/server.js\"",
    "start:prod": "bnr start:prod -s",
    "build:dev": "node ./scripts/build.js development",
    "build:prod": "node ./scripts/build.js production",
    "build:server:dev": "node ./scripts/build.js development --target server",
    "build:server:prod": "node ./scripts/build.js production --target server",
    "build:client:dev": "node ./scripts/build.js development --target client",
    "build:client:prod": "node ./scripts/build.js production --target client",
    "watch:server:dev": "node ./scripts/build.js development --target server --watch",
    "watch:server:prod": "node ./scripts/build.js production --target server --watch",
    "watch:client:dev": "node ./scripts/build.js development --target client --watch",
    "watch:client:prod": "node ./scripts/build.js production --target client --watch",
    "test": "karma start"
  },
  "betterScripts": {
    "start:prod": {
      "command": "npm run build:prod --silent && node ./dist/server/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Tobias Walle",
  "license": "ISC",
