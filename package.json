{
  "name": "typed-react-base",
  "version": "1.0.0",
  "description": "Isomorphic React with Typescript, Redux, Wepack and Express",
  "main": "dist/server/server.js",
  "author": "Tobias Walle",
  "homepage": "https://github.com/TobiasWalle/typed-react-base",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "start": "npm run --silent start:dev",
    "start:dev": "node ./scripts/build.js development --watch --target server --callback \"node ./dist/server/server.js\"",
    "start:prod": "node ./scripts/build.js production --watch --callback \"node ./dist/server/server.js\" --force-restart",
    "build:dev": "node ./scripts/build.js development",
    "build:prod": "node ./scripts/build.js production",
    "build:server:dev": "node ./scripts/build.js development --target server",
    "build:server:prod": "node ./scripts/build.js production --target server",
    "build:client:dev": "node ./scripts/build.js development --target client",
    "build:client:prod": "node ./scripts/build.js production --target client",
    "watch:server:dev": "node ./scripts/build.js development --target server --watch",
    "watch:server:prod": "node ./scripts/build.js production --target server --watch",
    "watch:client:dev": "node ./scripts/build.js development --target client --watch",
    "watch:client:prod": "node ./scripts/build.js production --target client --watch",
    "test": "npm run --silent test:dev",
    "test:dev": "karma start --single-run",
    "test:prod": "NODE_ENV=production karma start --single-run",
    "test:e2e": "protractor protractor.conf.js",
    "lint": "tslint src/**/*.ts{,x}",
    "check": "npm run lint && npm run test"
  },
  "devDependencies": {
    "@types/enzyme": "^2.7.6",
    "@types/express": "^4.0.35",
    "@types/jasmine": "^2.5.46",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.11",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.38",
    "@types/react-router-redux": "^5.0.0",
    "@types/webpack": "^2.2.12",
    "@types/webpack-dev-server": "^1.12.6",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.0",
    "file-loader": "^0.11.1",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "minimist": "^1.2.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.3",
    "protractor": "^5.1.1",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools-extension": "^2.13.0",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.0",
    "source-map-support": "^0.4.14",
    "style-loader": "^0.16.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "tslint-react": "^2.5.0",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "@types/react-router-dom": "^4.0.3",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "http-proxy": "^1.16.2",
    "isomorphic-style-loader": "^1.1.0",
    "isomorphic-style-loader-utils": "^1.0.0",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.1",
    "normalize-scss": "^6.0.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-helmet": "^5.0.1",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "react-router-server": "^4.0.0",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "redux-typed-ducks": "^0.3.0",
    "rxjs": "^5.3.0",
    "serialize-javascript": "^1.3.0",
    "ts-helpers": "^1.1.2"
  }
}
