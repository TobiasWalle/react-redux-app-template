{
  "name": "rush",
  "version": "1.0.0",
  "description": "Light speed development of web applications with React, Typescript, Webpack, Server Side Rendering, Tree Shaking and a lot more",
  "main": "dist/server/server.js",
  "author": "Tobias Walle",
  "homepage": "https://github.com/TobiasWalle/typed-react-base",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "start": "npm run --silent start:dev",
    "start:dev": "run-p start:api:dev start:server:dev",
    "start:prod": "run-p start:api:prod start:server:prod",
    "start:server:dev": "node ./scripts/build.js development --watch --target server --callback \"node ./dist/server/server.js\"",
    "start:api:dev": "node ./scripts/build.js development --watch --target api --callback \"node ./dist/api/api.js\"",
    "start:server:prod": "node ./scripts/build.js production --watch --target server,client --callback \"node ./dist/server/server.js\" --force-restart",
    "start:api:prod": "node ./scripts/build.js production --watch --target api --callback \"node ./dist/api/api.js\" --force-restart",
    "run:server": "node ./dist/server/server.js",
    "run:api": "node ./dist/api/api.js",
    "build:dev": "node ./scripts/build.js development",
    "build:prod": "node ./scripts/build.js production",
    "build:server:dev": "node ./scripts/build.js development --target server",
    "build:server:prod": "node ./scripts/build.js production --target server",
    "build:api:dev": "node ./scripts/build.js development --target api",
    "build:api:prod": "node ./scripts/build.js production --target api",
    "build:client:dev": "node ./scripts/build.js development --target client",
    "build:client:prod": "node ./scripts/build.js production --target client",
    "analyze": "ANALYZE=true webpack --config ./webpack/webpack.client.config.production.js",
    "test": "npm run --silent test:dev",
    "test:dev": "karma start --single-run",
    "test:prod": "NODE_ENV=production karma start --single-run",
    "test:e2e": "protractor protractor.conf.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "tslint \"src/**/*.ts{,x}\" -c './tslint.json' --project './tsconfig.json'",
    "lint:fix": "tslint \"src/**/*.ts{,x}\" -c './tslint.json' --fix --project './tsconfig.json'",
    "check": "run-p lint test"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.1",
    "@types/express": "^4.0.36",
    "@types/history": "^4.6.0",
    "@types/jasmine": "^2.5.53",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.9",
    "@types/react": "15.0.4",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.45",
    "@types/react-router-dom": "^4.0.6",
    "@types/react-router-redux": "^5.0.3",
    "@types/source-map-support": "^0.4.0",
    "@types/webpack": "^3.0.2",
    "@types/webpack-dev-server": "^2.4.0",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "file-loader": "^0.11.2",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.4",
    "minimist": "^1.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "protractor": "^5.1.2",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.0",
    "source-map-support": "^0.4.14",
    "style-loader": "^0.18.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.1.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.5.1",
    "webpack-manifest-plugin": "^1.1.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "history": "^4.6.3",
    "http-proxy": "^1.16.2",
    "isomorphic-style-loader": "^3.0.0",
    "isomorphic-style-loader-utils": "^1.0.7",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "normalize-scss": "^7.0.0",
    "react": "next",
    "react-dom": "next",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-server": "^4.2.1",
    "redux": "^3.7.1",
    "redux-observable": "^0.14.1",
    "redux-typed-ducks": "^0.3.0",
    "rxjs": "^5.4.2",
    "serialize-javascript": "^1.3.0",
    "server-destroy": "^1.0.1",
    "typescript-logging": "^0.4.0"
  }
}
